    "terminal": {
      "profiles": [
        {
          "id": "zephyr-ide.terminal-profile",
          "icon": "terminal",
					"title": "Atmosic IDE Terminal"
        }
      ]
    },
    "debuggers": [
      {
        "type": "zephyr-ide",
				"label": "Atmosic IDE",
        "initialConfigurations": [
          {
						"name": "Atmosic IDE: Debug",
            "cwd": "${workspaceFolder}",
						"gdbPath": "${command:atmosic-ide.get-gdb-path}",
            "executable": "${command:zephyr-ide.get-active-build-path}/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "bmp",
            "interface": "swd",
            "device": "STM32F103",
            "BMPGDBSerialPort": "${config:bmp_serial_port}",
            "preLaunchCommands": [
              "monitor rtt enable"
            ]
          },
          {
						"name": "Atmosic IDE: Attach",
            "cwd": "${workspaceFolder}",
						"gdbPath": "${command:atmosic-ide.get-gdb-path}",
            "executable": "${command:zephyr-ide.get-active-build-path}/zephyr/zephyr.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "bmp",
            "interface": "swd",
            "device": "STM32F103",
            "BMPGDBSerialPort": "${config:bmp_serial_port}",
            "preLaunchCommands": [
              "monitor rtt enable"
            ]
          }
        ],
        "configurationSnippets": [
          {
						"label": "Atmosic IDE: Debug",
            "description": "Basic Cortex Debug example with blackmagic probe",
            "body": {
							"name": "Atmosic IDE: Debug",
              "cwd": "^\"\\${workspaceFolder}\"",
							"gdbPath": "^\"\\${command:atmosic-ide.get-gdb-path}\"",
              "executable": "^\"\\${command:zephyr-ide.get-active-build-path}/zephyr/zephyr.elf\"",
              "request": "launch",
              "type": "cortex-debug",
              "servertype": "bmp",
              "interface": "swd",
              "device": "STM32F103",
              "BMPGDBSerialPort": "^\"\\${config:bmp_serial_port}\"",
              "preLaunchCommands": [
                "monitor rtt enable"
              ]
            }
          },
          {
						"label": "Atmosic IDE: Attach",
            "description": "Basic Cortex Debug attach example with blackmagic probe",
            "body": {
							"name": "Atmosic IDE: Attach",
              "cwd": "^\"\\${workspaceFolder}\"",
							"gdbPath": "^\"\\${command:atmosic-ide.get-gdb-path}\"",
              "executable": "^\"\\${command:zephyr-ide.get-active-build-path}/zephyr/zephyr.elf\"",
              "request": "launch",
              "type": "cortex-debug",
              "servertype": "bmp",
              "interface": "swd",
              "device": "STM32F103",
              "BMPGDBSerialPort": "^\"\\${config:bmp_serial_port}\"",
              "preLaunchCommands": [
                "monitor rtt enable"
              ]
            }
          },
          {
						"label": "Atmosic IDE: Debug Select",
            "description": "An example where a user will select a build each time the configuration is run",
            "body": {
							"name": "Atmosic IDE: Debug Select",
              "cwd": "^\"\\${workspaceFolder}\"",
							"gdbPath": "^\"\\${command:atmosic-ide.get-gdb-path}\"",
              "executable": "^\"\\${command:zephyr-ide.select-active-build-path}/zephyr/zephyr.elf\"",
              "request": "launch",
              "type": "cortex-debug",
              "servertype": "bmp",
              "interface": "swd",
              "device": "STM32F103",
              "BMPGDBSerialPort": "^\"\\${config:bmp_serial_port}\"",
              "preLaunchCommands": [
