{
	"name": "atmosic-ide",
	"displayName": "Atmosic IDE",
	"description": "A VS Code extension for Atmosic IDE",
	"version": "1.7.2",
  "icon": "media/logo.png",
  "keywords": [
    "zephyr",
    "ide",
    "vscode",
    "extension",
    "development environment",
    "zephyr project",
    "rtos"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mylonics/zephyr-ide.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
		"atmosic-ide.get-active-project-name",
		"atmosic-ide.get-active-project-path",
		"atmosic-ide.get-active-build-path",
		"atmosic-ide.get-active-build-board-path",
		"atmosic-ide.select-active-build-path",
		"atmosic-ide.get-gdb-path",
		"atmosic-ide.get-toolchain-path",
		"atmosic-ide.get-zephyr-ide-json-variable",
		"atmosic-ide.get-active-project-variable",
		"atmosic-ide.get-active-build-variable",
		"atmosic-ide.get-active-board-name",
    "workspaceContains:.vscode/zephyr-ide.json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "taskDefinitions": [
      {
				"type": "atmosic-ide"
      }
    ],
    "configuration": {
      "type": "object",
			"title": "Atmosic IDE: Projects",
      "properties": {
				"atmosic-ide.projects": {
          "type": [
            "any",
            "null",
            "object"
          ],
          "default": null,
					"description": "[deprecated] Atmosic IDE Project Configuration."
        },
				"atmosic-ide.use-zephyr-ide-json": {
          "type": [
            "boolean"
          ],
          "default": false,
					"description": "[deprecated] Allows use of separater json file to store Atmosic IDE Project Configuration."
        },
				"atmosic-ide.tools_directory": {
          "type": [
            "string"
          ],
          "default": null,
          "description": "Manually specifiy a tools directory for sdk download/install and global zephyr install location"
        }
      }
    },
    "keybindings": [
      {
				"command": "atmosic-ide.build",
        "key": "ctrl+alt+B",
        "mac": "ctrl+alt+B"
      },
      {
				"command": "atmosic-ide.flash",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p"
      }
    ],
    "viewsWelcome": [
      {
        "view": "welcome",
				"contents": "In order to use the Atmosic IDE Extension it needs to be fully Initialized.\n[Initialize Extension](command:atmosic-ide.setup-workspace-init)\n Elements of this process may be run individually as well\n This is a 5 step process that can be run by git features, you can open a folder containing a git repository or clone from a URL.\n[Open Folder](command:vscode.openFolder)\n[Clone Repository](command:git.clone)\nTo learn more about how to use git and source control in VS Code [read our docs](https://aka.ms/vscode-scm).",
        "when": "config.git.enabled && git.state == initialized && workbenchState == empty"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
					"id": "atm",
					"title": "Atmosic IDE",
					"icon": "media/icon26.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
					"command": "atmosic-ide.add-project",
          "group": "navigation",
					"when": "view == atmosicIdeProjects"
        },
        {
					"command": "atmosic-ide.create-project",
          "group": "navigation",
					"when": "view == atmosicIdeProjects"
        },
        {
					"command": "atmosic-ide.load-projects-from-file",
          "group": "navigation",
					"when": "view == atmosicIdeProjects"
        },
        {
					"command": "atmosic-ide.save-projects-to-file",
          "group": "navigation",
					"when": "view == atmosicIdeProjects"
        },
        {
					"command": "atmosic-ide.disable-automatic-project-target",
					"when": "view == atmosicIdeActiveProject"
        },
        {
					"command": "atmosic-ide.enable-automatic-project-target",
					"when": "view == atmosicIdeActiveProject"
        }
      ]
    },
    "views": {
			"atm": [
        {
					"id": "atmosicIdeExtensionSetup",
          "name": "Extension Setup",
          "icon": "media/logo_small.jpg",
					"contextualTitle": "Atmosic IDE",
          "type": "webview"
        },
        {
					"id": "atmosicIdeProjects",
          "name": "Projects",
          "icon": "media/logo_small.jpg",
					"contextualTitle": "Atmosic IDE Projects",
          "type": "webview"
        },
        {
					"id": "atmosicIdeProjectStatus",
          "name": "Project Config",
          "icon": "media/logo_small.jpg",
					"contextualTitle": "Atmosic IDE Projects",
          "type": "webview"
        },
        {
					"id": "atmosicIdeActiveProject",
          "name": "Active Project",
          "icon": "media/logo_small.jpg",
					"contextualTitle": "Atmosic IDE Active Project",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
				"command": "atmosic-ide.init-workspace",
